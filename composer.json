{
	"name": "izzatfirdaus/umamusume-tracker-app",
	"description": "Uma Musume Career Tracker - Laravel 11+ Migration",
	"type": "project",
	"require": {
		"php": "^8.2",
		"laravel/framework": "^11.0",
		"laravel/sanctum": "^4.0",
		"maatwebsite/excel": "^3.1"
	},
	"require-dev": {
		"nunomaduro/collision": "^8.1",
		"laravel/boost": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	}
}
